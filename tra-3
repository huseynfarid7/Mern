    <title>Document</title>
    <script>
        function digitize(n) {
            const digitsArray = String(n).split('').map(Number);
            const reversedArray = digitsArray.reverse();
            return reversedArray;
        }
        console.log(digitize(35231));  
    </script>
</head>
<body>
    <h2>view the console</h2>
</body>
